{
  "name": "wxtm-bridge-backend",
  "version": "1.0.0",
  "description": "NestJs backend for WXTM Bridge",
  "private": true,
  "license": "MIT License",
  "scripts": {
    "build": "nest build",
    "esbuild": "node compileBundle.mjs",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "npm run build && npm run migrations:run && nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "typecheck": "tsc --noEmit",
    "test": "jest --runInBand",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "typeorm": "typeorm --dataSource dist/database/database.data-source.js",
    "migrations:run": "npm run typeorm migration:run -t false",
    "migrations:generate": "npm run typeorm migration:generate",
    "main": "docker exec -it wxtm-bridge-backend-app-1 bash",
    "api:generate": "ts-node generate-api.ts"
  },
  "dependencies": {
    "@nestjs/common": "^10.3.4",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^10.3.4",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/swagger": "^7.3.1",
    "@nestjs/typeorm": "^10.0.2",
    "@types/uuid": "^10.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "typeorm": "^0.3.20",
    "pg": "^8.8.0",
    "uuid": "^10.0.0",
    "@vendia/serverless-express": "^4.3.4"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/express": "^4.17.17",
    "@types/factory-girl": "^5.0.12",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.3.1",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "esbuild": "^0.23.1",
    "esbuild-plugin-tsc": "^0.4.0",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "factory-girl": "^5.0.4",
    "jest": "^29.5.0",
    "openapi-typescript-codegen": "^0.29.0",
    "prettier": "^3.0.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3",
    "aws-lambda": "^1.0.6",
    "@types/aws-lambda": "^8.10.75",
    "@aws-sdk/client-codedeploy": "3.435.0"
  }
}
